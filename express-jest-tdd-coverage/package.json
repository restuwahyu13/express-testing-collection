{
	"name": "express-tdd-jest-coverage",
	"version": "1.0.0",
	"description": "example express tdd test with jest using coverage",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"test": "cross-env NODE_ENV=test jest --forceExit",
		"test-watch": "cross-env NODE_ENV=test jest --watchAll --forceExit",
		"test-coverage": "cross-env NODE_ENV=test jest --forceExit --coverage"
	},
	"repository": {
		"type": "git",
		"url": "restuwahyu13"
	},
	"keywords": [
		"node",
		"mock",
		"express",
		"mock"
	],
	"author": "restuwahyu13",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"http-status-codes": "^2.1.4",
		"mongoose": "^5.12.10"
	},
	"devDependencies": {
		"@types/jest": "^25.2.3",
		"cross-env": "^7.0.3",
		"dotenv": "^9.0.2",
		"faker": "^5.5.3",
		"is-any-type": "0.0.3",
		"jest": "^25.5.4",
		"morgan": "^1.10.0",
		"node-mocks-http": "^1.10.1",
		"nodemon": "^2.0.7",
		"supertest": "^6.1.3"
	}
