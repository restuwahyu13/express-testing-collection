{
	"name": "express-tdd-mocha-ts",
	"version": "1.0.0",
	"description": "example express tdd test with mocha and typescript",
	"main": "server.js",
	"scripts": {
		"cleanup": "rimraf dist",
		"compiler": "tsc",
		"build": "npm run cleanup && npm run compiler",
		"start": "node dist/server.js",
		"dev": "tsnd --poll --respawn --quiet --watch src/**/*.{ts} --files -H -T server.ts",
		"test": "cross-env NODE_ENV=test mocha",
		"test-watch": "cross-env NODE_ENV=test mocha -w"
	},
	"repository": {
		"type": "git",
		"url": "restuwahyu13"
	},
	"keywords": [
		"node",
		"mock",
		"express",
		"typescript",
		"tdd",
		"mocha"
	],
	"author": "restuwahyu13",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"http-status-codes": "^2.1.4",
		"mongoose": "^5.12.10",
		"q": "^1.5.1"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.0",
		"@types/chai": "^4.2.18",
		"@types/chai-http": "^4.2.0",
		"@types/compression": "^1.7.0",
		"@types/cors": "^2.8.10",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.12",
		"@types/faker": "^5.5.5",
		"@types/helmet": "^4.0.0",
		"@types/http-status-codes": "^1.2.0",
		"@types/mocha": "^8.2.2",
		"@types/mongoose": "^5.10.5",
		"@types/morgan": "^1.9.2",
		"@types/node": "^15.6.1",
		"@types/q": "^1.5.4",
		"@types/rimraf": "^3.0.0",
		"@types/sinon": "^10.0.1",
		"@types/supertest": "^2.0.11",
		"chai": "^4.3.4",
		"chai-http": "^4.3.0",
		"cross-env": "^7.0.3",
		"dotenv": "^9.0.2",
		"faker": "^5.5.3",
		"is-any-type": "0.0.3",
		"mocha": "^8.4.0",
		"morgan": "^1.10.0",
		"node-mocks-http": "^1.10.1",
		"rimraf": "^3.0.2",
		"sinon": "^11.1.1",
		"ts-node": "^10.0.0",
		"ts-node-dev": "^1.1.6",
		"tslib": "^2.2.0",
		"typescript": "^4.3.2"
	}
}
